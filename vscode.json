{
  "launch": {

      "configurations": [],
      "compounds": []
  },
  "editor.tabSize": 2, // tab键的大小
  "editor.codeActionsOnSave": { //保存代码的时候自动启动ESlint插件修复当前focus代码
      "source.fixAll.eslint": true,
  },
  "eslint.codeAction.showDocumentation": {
      "enable": true
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "emmet.includeLanguages": { //jS里JSX自动补全代码
      "javascript": "javascriptreact"
  },
  "terminal.integrated.profiles.windows": { // 配置 VSCode终端
      "Shell": {
          "source": "PowerShell",
          "icon": "terminal-powershell"
      },
      // "Command Prompt": {
      //     "path": [
      //         "${env:windir}\\Sysnative\\cmd.exe",
      //         "${env:windir}\\System32\\cmd.exe"
      //     ],
      //     "args": [],
      //     "icon": "terminal-cmd"
      // },
      "Git": { // 终端显示地title
          "path": "C:\\Program Files\\Git\\bin\\bash.exe",
          "args": [], // 启动终端地参数
      }
  },
  "terminal.integrated.defaultProfile.windows": "Git",
  "terminal.integrated.tabs.enabled": true,
  "workbench.iconTheme": "material-icon-theme",
  "files.eol": "\n", //默认创建地文件都是 LF 格式
}

